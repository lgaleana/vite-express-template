{
  "name": "server",
  "version": "1.0.0",
  "description": "Express TypeScript server",
  "type": "module",
  "main": "dist/index.js",
  "engines": {
    "node": ">=22.0.0"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx --watch src/index.ts",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit",
    "lint": "echo \"Add ESLint configuration if needed\"",
    "dev:instrumented": "tsx tracing/dev-instrumented.ts 2>&1 | tee tracing/tracing.log"
  },
  "keywords": [
    "express",
    "typescript",
    "api",
    "esm",
    "express5"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@supabase/supabase-js": "^2.53.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^5.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-typescript": "^7.24.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.1",
    "@types/node": "^22.13.11",
    "chokidar": "^4.0.1",
    "ts-morph": "^26.0.0",
    "tsx": "^4.19.2",
    "typescript": "^5.8.2"
  }
}
